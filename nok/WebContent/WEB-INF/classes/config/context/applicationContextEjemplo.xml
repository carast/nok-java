<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:aop="http://www.springframework.org/schema/aop"
    xmlns:tx="http://www.springframework.org/schema/tx"
    xsi:schemaLocation="
    http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
    http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd
    http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd">
    	
    	
    	<!-- DAO -->
	<bean id="ejemploDAO" class="mx.com.nok.ejemplo.dao.ibatis.EjemploDAOImpl">	
            <property name="sqlMapClient" ref="sqlMap"/>
	</bean>
	<!-- Business -->
    <bean id="ejemploBusiness" class="mx.com.nok.ejemplo.model.business.EjemploBusiness">
		<property name="empleadoDAO" ref="empleadoDAO"/>
		<property name="usuarioDAO" ref="usuarioDAO"/>
		<property name="perfilDAO" ref="perfilDAO"/>
		
	</bean>
	<!-- Action -->
    <bean id="ejemploAction" class="mx.com.nok.ejemplo.action.EjemploAction">
		<property name="empleadoService" ref="empleadoBusiness"/>
		
    </bean>
	
</beans>	